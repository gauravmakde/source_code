{
    "dag_id": "credit_account_teradata",
    "dag_schedule": "0 */3 * * *",
    "stages": [
        "credit_account_kafka_to_teradata_stg_stage_0"
    ],
    "credit_account_kafka_to_teradata_stg_stage_0": {
        "kafka_to_teradata_stg_job": {
            "spark_engine": {
                "spark_run_mode": "medium",
                "scripts": [
                    "sparkSql/credit_account_kafka_to_teradata_stg.sql"
                ],
                "input_details": [
                    {
                        "conn_name": "__KAFKA_CONFLUENT_CLOUD_OAUTH__",
                        "sql_table_reference": "kafka_customer_credit_account_analytical_avro",
                        "data_source_name": "customer-credit-account-analytical-avro"
                    }
                ],
                "output_details": [
                    {
                        "conn_name": "__CREDIT_TERADATA_NAP_STG__",
                        "sql_table_reference": "credit_account_ldg",
                        "data_source_name": "CREDIT_ACCOUNT_LDG"
                    },
                    {
                        "conn_name": "__CREDIT_TERADATA_NAP_STG__",
                        "sql_table_reference": "credit_account_bank_card_ldg",
                        "data_source_name": "CREDIT_ACCOUNT_BANK_CARD_LDG"
                    }
                ]
            }
        },
        "teradata_stg_to_teradata_fct_job": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "__CREDIT_TERADATA_NAP_FCT__",
                        "scripts": [
                            "teradata/credit_account_teradata_stg_to_teradata_fct.sql"
                        ]
                    }
                ]
            }
        }
    }
}