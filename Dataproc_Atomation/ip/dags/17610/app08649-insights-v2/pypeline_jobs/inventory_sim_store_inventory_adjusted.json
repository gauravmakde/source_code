{
  "dag_id": "inventory_sim_store_inventory_adjusted",
  "dag_schedule": "0 8 * * *",
  "prod_email_on_failure":"TECH_NAP_SUPPLYCHAIN_OUTBOUND@nordstrom.com,nap-sco-high-pri@nordstrom.pagerduty.com",
  "nonprod_email_on_failure": "TECH_NAP_SUPPLYCHAIN_OUTBOUND@nordstrom.com",
  "stages": [
    "main_v1"
  ],
  "main_v1": {
    "job_1_td_batch_start": {
      "teradata_engine": {
        "input_details": [
          {
            "conn_name": "__TERADATA_NAP_FCT__",
            "scripts": [
              "inventory_sim_store_inventory_adjusted/batch_start.sql"
            ]
          }
        ]
      }
    },
    "job_2_load_avro_td_stg": {
      "spark_engine": {
        "spark_run_mode": "medium",
        "scripts": [
          "inventory_sim_store_inventory_adjusted/kafka_avro_to_td_stg.sql"
        ],
        "input_details": [
          {
            "conn_name": "KAFKA_SOURCE_INVENTORYEVENT_ADJUSTED_STORE_EVENT",
            "sql_table_reference": "kafka_inventory_return_store_inventory_sim_adjusted",
            "data_source_name": "inventory-store-inventoryevent-avro"
          }
        ],
        "output_details": [
          {
            "conn_name": "__TERADATA_NAP_STG__",
            "sql_table_reference": "store_inventory_sim_adjusted_event_ldg",
            "data_source_name": "INVENTORY_SIM_STORE_INVENTORY_ADJUSTED_LDG"
          }
        ]
      }
    },
    "job_3_td_stg_to_fact": {
      "teradata_engine": {
        "input_details": [
          {
            "conn_name": "__TERADATA_NAP_FCT__",
            "scripts": [
              "inventory_sim_store_inventory_adjusted/td_stg_to_td_fact.sql"
            ]
          }
        ]
      }
    },
    "job_31_teradata_fact_dqc": {
      "teradata_engine": {
        "retries": 0,
        "input_details": [
          {
            "conn_name": "__TERADATA_NAP_FCT__",
            "scripts": [
              "inventory_sim_store_inventory_adjusted/td_fact_dqc.sql"
            ]
          }
        ]
      }
    },
    "job_4_td_batch_end": {
      "teradata_engine": {
        "input_details": [
          {
            "conn_name": "__TERADATA_NAP_FCT__",
            "scripts": [
              "inventory_sim_store_inventory_adjusted/batch_end.sql"
            ]
          }
        ]
      }
    }
  }
}

