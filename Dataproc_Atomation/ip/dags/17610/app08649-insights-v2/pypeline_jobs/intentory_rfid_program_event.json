{
  "dag_id": "inventory_rfid_program_event",
  "dag_schedule": "0 8 * * *",
  "prod_email_on_failure":"TECH_NAP_SUPPLYCHAIN_OUTBOUND@nordstrom.com,nap-sco-high-pri@nordstrom.pagerduty.com",
  "nonprod_email_on_failure": "TECH_NAP_SUPPLYCHAIN_OUTBOUND@nordstrom.com",
  "stages": [
    "main_3"
  ],
  "main_3": {
    "job_1_td_batch_start": {
      "teradata_engine": {
        "input_details": [
          {
            "conn_name": "__TERADATA_NAP_FCT__",
            "scripts": [
              "inventory_rfid_program_event/batch_start.sql"
            ]
          }
        ]
      }
    },
    "job_2_load_avro_td_stg": {
      "spark_engine": {
        "spark_run_mode": "medium",
        "scripts": [
          "inventory_rfid_program_event/kafka_avro_to_td_stg.sql"
        ],
        "input_details": [
          {
            "conn_name": "__KAFKA_SOURCE_INVENTORY_RFID_PROGRAM_SKU_ADDED_EVENT__",
            "sql_table_reference": "kafka_inventory_rfid_program_sku_added",
            "data_source_name": "inventory-rfid-program-avro"
          },
          {
            "conn_name": "__KAFKA_SOURCE_INVENTORY_RFID_PROGRAM_SKU_REMOVED_EVENT__",
            "sql_table_reference": "kafka_inventory_rfid_program_sku_removed",
            "data_source_name": "inventory-rfid-program-avro"
          }
        ],
        "output_details": [
          {
            "conn_name": "__TERADATA_NAP_STG__",
            "sql_table_reference": "inventory_rfid_program_event_ldg",
            "data_source_name": "INVENTORY_RFID_PROGRAM_EVENT_LDG"
          }
        ]
      }
    },
    "job_3_td_stg_to_fact": {
      "teradata_engine": {
        "input_details": [
          {
            "conn_name": "__TERADATA_NAP_FCT__",
            "scripts": [
              "inventory_rfid_program_event/td_stg_to_td_fact.sql"
            ]
          }
        ]
      }
    },
    "job_31_teradata_fact_dqc": {
      "teradata_engine": {
        "retries": 0,
        "input_details": [
          {
            "conn_name": "__TERADATA_NAP_FCT__",
            "scripts": [
              "inventory_rfid_program_event/td_fact_dqc.sql"
            ]
          }
        ]
      }
    },
    "job_4_td_batch_end": {
      "teradata_engine": {
        "input_details": [
          {
            "conn_name": "__TERADATA_NAP_FCT__",
            "scripts": [
              "inventory_rfid_program_event/batch_end.sql"
            ]
          }
        ]
      }
    }
  }
}

