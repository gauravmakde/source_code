{
    "dag_id": "merch_main_mp_summary",
    "dag_schedule": "00 10 * * *",
    "timezone": "US/Pacific",
    "prod_email_on_failure": "dsa-app09340-marketplace@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "asiyah.fox@nordstrom.com",
    "stages": [
        "run_teradata_sql_stg_00"
    ],
    "sql_env_vars": {
        "environment_schema": "T2DL_DAS_INSIGHTS_DELIVERY_SECURE"
    },
    "run_teradata_sql_stg_00": {
        "job_0": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "__TERADATA_T2DL_NAP_IDS_BATCH__",
                        "scripts": [
                            "marketplace/ma_report_main_mp_summary.sql"
                        ]
                    }
                ]
            }
        },
        "job_0_refresh_tableau": {
            "tableau_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TABLEAU_MA__",
                        "source_type": "datasource",
                        "datasource_name": "DNU_mp_summary",
                        "project_name": "Data Sources"
                    }
                ],
                "output_details": []
            }
        }
    }
}