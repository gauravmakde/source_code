{
    "dag_id": "merch_main_price_band_weekly",
    "dag_schedule": "01 11 * * 0",
    "timezone": "US/Pacific",
    "prod_email_on_failure": "ACE.promo@nordstrom.com,dsa-app09094-price-band-performance@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "jevon.barlas@nordstrom.com",
    "stages": [
        "run_teradata_sql_stg_00"
    ],
    "sql_env_vars": {
        "environment_schema": "T2DL_DAS_IN_SEASON_MANAGEMENT_REPORTING"
    },
    "run_teradata_sql_stg_00": {
        "job_0_populate_weekly_table": {
            "teradata_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TERADATA_T2DL_NAP_AIS_BATCH__",
                        "scripts": [
                            "price_band/ma_pipeline_prod_main_price_band_weekly.sql"
                        ]
                    }
                ]
            }
        },
        "job_1_refresh_tableau_extract": {
            "tableau_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TABLEAU_MA__",
                        "source_type": "datasource",
                        "datasource_name": "Price Band Monthly",
                        "project_name": "Data Sources"
                    }
                ],
                "output_details": []
            }
        }
    }
}