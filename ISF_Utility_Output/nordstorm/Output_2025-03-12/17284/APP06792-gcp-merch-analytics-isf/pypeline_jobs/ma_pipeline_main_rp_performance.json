{
    "dag_id": "merch_main_rp_performance",
    "dag_schedule": "15 08 * * 1-5",
    "timezone": "US/Pacific",
    "prod_email_on_failure": "ACE.Promo@nordstrom.com, dsa-app09156-rp-performance@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "ACE.Promo@nordstrom.com",
    "stages": [
        "run_teradata_sql_stg_00"
    ],
    "sql_env_vars": {
        "environment_schema": "T2DL_DAS_IN_SEASON_MANAGEMENT_REPORTING"
    },
    "run_teradata_sql_stg_00": {
        "job_0": {
            "teradata_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TERADATA_T2DL_NAP_AIS_BATCH__",
                        "scripts": [
                            "rp_performance/ma_pipeline_main_rp_performance.sql"
                        ]
                    }
                ]
            }
        },
        "job_0_refresh_tableau": {
            "tableau_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TABLEAU_MA__",
                        "source_type": "datasource",
                        "datasource_name": "RP Performance Self Service",
                        "project_name": "RP Performance"
                    }
                ],
                "output_details": []
            }
        }
    }
}