{
    "dag_id": "merch_main_customer_brand_reporting",
    "dag_schedule": "00 06 2 * *",
    "timezone": "US/Pacific",
    "prod_email_on_failure": "dsa-app08908-merchcustomerreporting@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "rasagnya.avala@nordstrom.com",
    "stages": [
        "run_teradata_sql_stg_00",
        "refresh_merch_customer_base",
        "refresh_merch_customer_demographic"
    ],
    "sql_env_vars": {
        "environment_schema": "T2DL_DAS_CUS"
    },
    "run_teradata_sql_stg_00": {
        "job_0": {
            "teradata_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TERADATA_T2DL_NAP_CUS_BATCH__",
                        "scripts": [
                            "merch_customer_reporting/ma_pipeline_main_merch_customer_brand_base.sql"
                        ]
                    }
                ]
            }
        },
        "refresh_merch_customer_base": {
            "tableau_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TABLEAU_MA__",
                        "source_type": "datasource",
                        "datasource_name": "Merch x Customer Base (DNU)",
                        "project_name": "Data Sources"
                    }
                ],
                "output_details": []
            }
        },
        "refresh_merch_customer_demographic": {
            "tableau_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TABLEAU_MA__",
                        "source_type": "datasource",
                        "datasource_name": "Merch x Customer Demographic (DNU)",
                        "project_name": "Data Sources"
                    }
                ],
                "output_details": []
            }
        }
    }
}