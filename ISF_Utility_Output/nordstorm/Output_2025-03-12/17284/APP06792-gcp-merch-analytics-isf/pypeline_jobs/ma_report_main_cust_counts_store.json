{
    "dag_id": "merch_main_cust_counts_store",
    "dag_schedule": "01 19 * * 0",
    "timezone": "US/Pacific",
    "prod_email_on_failure": "ACE.promo@nordstrom.com, dsa-app09268-merch-analytics-store-reporting@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "sara.scott@nordstrom.com",
    "stages": [
        "run_teradata_sql_stg_00"
    ],
    "sql_env_vars": {
        "environment_schema": "T2DL_DAS_IN_SEASON_MANAGEMENT_REPORTING",
        "start_date": "((CURRENT_DATE - 1)  - INTERVAL '14' DAY)",
        "end_date": "(CURRENT_DATE - 1)"
    },
    "run_teradata_sql_stg_00": {
        "job_0_populate_weekly_table": {
            "teradata_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TERADATA_T2DL_NAP_AIS_BATCH__",
                        "scripts": [
                            "store_reporting/ma_pipeline_prod_main_cust_counts_store.sql"
                        ]
                    }
                ]
            }
        },
        "job_1_refresh_tableau_extract": {
            "tableau_engine": {
                "sensor_timeout_seconds": 10800,
                "input_details": [
                    {
                        "conn_name": "__TABLEAU_MA__",
                        "source_type": "workbook",
                        "workbook_name": "Store Customer Counts",
                        "project_name": "Store"
                    }
                ],
                "output_details": []
            }
        }
    }
}