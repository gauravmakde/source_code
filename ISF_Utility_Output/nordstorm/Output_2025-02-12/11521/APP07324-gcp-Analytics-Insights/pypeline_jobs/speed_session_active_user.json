{
    "dag_id": "session_active_user",
    "dag_schedule": "0 18 * * *",
    "prod_email_on_failure": "dsa-app08742-session-path-expanded-events-data-pipelines@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "jin.liu2@nordstrom.com",
    "stages": [
        "read_write_s3_td",
        "refresh_active_user_dashboard"
    ],
    "stage_dependencies": {
        "read_write_s3_td": [
            "refresh_active_user_dashboard"
        ]
    },
    "sql_env_vars": {
        "start_date": "current_date()-6",
        "end_date": "current_date()-2"
    },
    "read_write_s3_td": {
        "job_0_check_speed": {
            "python_executor": {
                "module": "python/s3_file_sensor",
                "version": "0.3.0",
                "k8s_cpu_limit": 4,
                "k8s_cpu_request": 3,
                "k8s_memory_limit": "6Gi",
                "k8s_memory_request": "2Gi",
                "args": [
                    "python/s3_file_sensor/main.py",
                    "--function",
                    "dir_check",
                    "--bucket_nonprod",
                    "",
                    "--key_nonprod",
                    "",
                    "--bucket_prod",
                    "tf-nap-prod-styling-intermediate",
                    "--key_prod",
                    "speed_tables/session_user_attributes/activity_date_partition=",
                    "--max_retry_minutes",
                    "60",
                    "--dir_check_date_diff",
                    "-3"
                ]
            }
        },
        "job_1_load_s3_data_to_output": {
            "spark_engine": {
                "spark_run_mode": "medium",
                "retries": "0",
                "pause_idle_cluster_termination": "before_app",
                "scripts": [
                    "speed/session_active_user_load.sql"
                ],
                "input_details": [],
                "output_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_DEG",
                        "sql_table_reference": "session_active_user_output",
                        "data_source_name": "session_active_user_ldg"
                    }
                ]
            }
        },
        "job_2_update_t2_table_from_landing_table": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_DEG",
                        "scripts": [
                            "speed/session_active_user.sql"
                        ]
                    }
                ]
            }
        }
    },
    "refresh_active_user_dashboard": {
        "job_0": {
            "tableau_engine": {
                "input_details": [
                    {
                        "conn_name": "__TABLEAU_NAP__",
                        "source_type": "workbook",
                        "workbook_name": "Active User - Digital Growth Performance",
                        "project_name": "Active User"
                    }
                ],
                "output_details": []
            }
        }
    }
}