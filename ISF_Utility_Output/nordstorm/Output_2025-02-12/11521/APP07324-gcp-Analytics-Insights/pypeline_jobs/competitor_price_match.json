{
    "dag_id": "competitor_price_match",
    "dag_schedule": "00 18 * * *",
    "prod_email_on_failure": "dsa-app09073-competitor-price-data@nordstrom.pagerduty.com, vamshi.medisetty@nordstrom.com",
    "nonprod_email_on_failure": "vamshi.medisetty@nordstrom.com",
    "sql_env_vars": {
        "start_date": "current_date()-4",
        "end_date": "current_date()",
        "year": "2024"
    },
    "stages": [
        "load_to_csv",
        "ldg_to_t2",
        "tpt_load_ldg"
    ],
    "stage_dependencies": {
        "load_to_csv": "tpt_load_ldg",
        "tpt_load_ldg": "ldg_to_t2"
    },
    "load_to_csv": {
        "job_1_load_data_to_csv": {
            "spark_engine": {
                "spark_run_mode": "medium",
                "retries": "0",
                "pause_idle_cluster_termination": "before_app",
                "scripts": [
                    "competitor_price_match/competitor_price_match_load.sql"
                ],
                "input_details": [],
                "output_details": [
                    {
                        "conn_name": "TPT_CSV_OUTPUT",
                        "sql_table_reference": "competitor_price_matching_ldg_output"
                    }
                ]
            }
        }
    },
    "tpt_load_ldg": {
        "job_2_load_data_to_t2_ldg": {
            "tpt_engine": {
                "retries": "0",
                "input_details": [
                    {
                        "conn_name": "TPT_PRI",
                        "delimiter": "1F",
                        "table_name": "competitor_price_match_dump_ldg",
                        "s3_file_path": "s3://{tpt_path}/competitor_price_matching_ldg_output/*.csv.gz",
                        "job_name": "competitor_price_match_dump_ldg"
                    }
                ],
                "output_details": []
            }
        }
    },
    "ldg_to_t2": {
        "job_3_update_t2_tables_from_landing_table": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_PRI",
                        "scripts": [
                            "competitor_price_match/competitor_price_match.sql"
                        ]
                    }
                ]
            }
        }
    }
}