{
    "dag_id": "sales_person_video_customer_journey_orderline",
    "dag_schedule": "0 18 * * *",
    "prod_email_on_failure": "Anfernee.Calloway@nordstrom.com",
    "nonprod_email_on_failure": "Anfernee.Calloway@nordstrom.com",
    "sql_env_vars": {
        "start_date": "current_date()-2",
        "end_date": "current_date() - 1"
    },
    "stages": [
        "load_to_csv",
        "ldg_to_t2",
        "tpt_load_ldg"
    ],
    "stage_dependencies": {
        "load_to_csv": "tpt_load_ldg",
        "tpt_load_ldg": "ldg_to_t2"
    },
    "load_to_csv": {
        "job_1_load_data_to_csv": {
            "spark_engine": {
                "spark_run_mode": "medium_no_broadcast",
                "retries": "0",
                "pause_idle_cluster_termination": "before_app",
                "scripts": [
                    "sales_person_video/customer_journey_orderline_load.sql"
                ],
                "input_details": [],
                "output_details": [
                    {
                        "conn_name": "TPT_CSV_OUTPUT",
                        "sql_table_reference": "customer_journey_orderline_landing_table"
                    }
                ]
            }
        }
    },
    "tpt_load_ldg": {
        "job_2_load_data_to_t2_ldg": {
            "tpt_engine": {
                "input_details": [
                    {
                        "conn_name": "TPT_SPV",
                        "delimiter": "1F",
                        "table_name": "customer_journey_orderline_landing",
                        "s3_file_path": "s3://{tpt_path}/customer_journey_orderline_landing_table/*.csv.gz",
                        "job_name": "customer_journey_orderline_landing"
                    }
                ],
                "output_details": []
            }
        }
    },
    "ldg_to_t2": {
        "job_3_update_t2_tables_from_landing_table": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_SPV",
                        "scripts": [
                            "sales_person_video/customer_journey_orderline.sql"
                        ]
                    }
                ]
            }
        }
    }
}