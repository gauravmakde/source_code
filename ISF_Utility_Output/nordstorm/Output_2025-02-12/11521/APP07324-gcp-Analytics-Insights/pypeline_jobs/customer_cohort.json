{
    "dag_id": "customer_cohort",
    "prod_email_on_failure": "dsa-app08742-session-path-expanded-events-data-pipelines@nordstrom.pagerduty.com, jin.liu2@nordstrom.com",
    "nonprod_email_on_failure": "jin.liu2@nordstrom.com",
    "stages": [
        "update_TD_table",
        "tpt_export",
        "update_hive"
    ],
    "sql_env_vars": {
        "end_date": "current_date()-5"
    },
    "stage_dependencies": {
        "update_TD_table": [
            "tpt_export"
        ],
        "tpt_export": [
            "update_hive"
        ]
    },
    "update_TD_table": {
        "job_1_run_sql_to_update_t2_table": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_DEG",
                        "scripts": [
                            "leapfrog_holdout/customer_cohort.sql"
                        ]
                    }
                ]
            }
        }
    },
    "tpt_export": {
        "job_0_tpt_export": {
            "tpt_engine": {
                "retries": "0",
                "input_details": [
                    {
                        "conn_name": "TPT_DEG",
                        "table_name": "customer_cohort",
                        "s3_file_path": "s3://{s3_bucket_root_var}/tpt_export/customer_cohort/customer_cohort.csv",
                        "sql_file_name": "leapfrog_holdout/tpt_export/customer_cohort.sql",
                        "text_delimiter_hexa_flag": "N",
                        "text_delimiter": ",",
                        "file_count": "1",
                        "quoted_flag": "Y",
                        "quoted_data": "'\\\"'"
                    }
                ],
                "output_details": []
            }
        }
    },
    "update_hive": {
        "job_0": {
            "spark_engine": {
                "spark_run_mode": "medium",
                "pause_idle_cluster_termination": "before_app",
                "scripts": [
                    "leapfrog_holdout/customer_cohort_hive.sql"
                ],
                "input_details": [],
                "output_details": []
            }
        }
    }
}