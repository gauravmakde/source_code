    "nonprod_email_on_failure": "soren.stime1@nordstrom.com",
    "dependencies": {
        "downstream": {
            "dags": [
                "session_experiment_variation_metadata_11521_ACE_ENG"
            ]
        }
    },
    "stages": [
        "run_hive_hive_session_experiment_variation_exposed_sql"
