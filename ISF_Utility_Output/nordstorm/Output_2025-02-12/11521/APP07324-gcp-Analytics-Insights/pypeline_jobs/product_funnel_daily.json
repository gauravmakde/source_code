    "prd_fun_dly_load_to_csv": {
        "job_1_load_data_to_csv": {
            "spark_engine": {
                "spark_run_mode": "medium",
                "retries": "0",
                "pause_idle_cluster_termination": "before_app",
                "scripts": [
                    "product_funnel/product_funnel_daily_load.sql"
                ],
                "input_details": [],
                "output_details": [
                    {
                        "conn_name": "TPT_CSV_OUTPUT",
                        "sql_table_reference": "product_funnel_daily_ldg_output"
                    }
                ]
            }
        }
    },
    "tpt_load_product_funnel_daily_ldg": {
        "tpt_prd_fun_dly_product_funnel_daily_ldg": {
            "tpt_engine": {
                "retries": "0",
                "input_details": [
                    {
                        "conn_name": "TPT_PFD",
                        "delimiter": "1F",
                        "table_name": "product_funnel_daily_ldg",
                        "s3_file_path": "s3://{tpt_path}/product_funnel_daily_ldg_output/*.csv.gz",
                        "job_name": "product_funnel_daily_ldg"
                    }
                ],
                "output_details": []
            }
        }
    },
    "prd_fun_dly_ldg_to_t2": {
        "job_3_update_t2_tables_from_landing_table": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_PFD",
                        "scripts": [
                            "product_funnel/product_funnel_daily.sql"
                        ]
                    }
                ]
            }
        }
    }
}