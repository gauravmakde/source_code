{
    "dag_id": "customer_attributes_table",
    "dag_schedule": "0 20 * * *",
    "prod_email_on_failure": "cia-app09142-customer-and-merch-attributes-to-object@nordstrom.pagerduty.com, jingnan.miao@nordstrom.com, Rathin.Deshpande@nordstrom.com",
    "nonprod_email_on_failure": "jingnan.miao@nordstrom.com, Rathin.Deshpande@nordstrom.com",
    "stages": [
        "update_final_customer_table",
        "tpt_export"
    ],
    "sql_env_vars": {
        "start_date": "current_date()-7",
        "end_date": "current_date()-1",
        "date_par": "current_date()-1"
    },
    "stage_dependencies": {
        "update_final_customer_table": [
            "tpt_export"
        ]
    },
    "update_final_customer_table": {
        "job_1_run_sql_to_update_t2_table": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_DMF",
                        "scripts": [
                            "digital_merch/customer_attributes_table.sql"
                        ]
                    }
                ]
            }
        }
    },
    "tpt_export": {
        "job_2_tpt_export": {
            "tpt_engine": {
                "retries": "0",
                "input_details": [
                    {
                        "conn_name": "TPT_DMF",
                        "table_name": "customer_attributes_td_to_s3",
                        "s3_file_path": "s3://{s3_bucket_root_var}/tpt_export/customer_attributes_table/customer_attributes_td_to_s3.csv",
                        "sql_file_name": "digital_merch/tpt_export/customer_attributes_table_td_to_s3.sql",
                        "text_delimiter_hexa_flag": "N",
                        "text_delimiter": ",",
                        "file_count": "4",
                        "quoted_flag": "Y",
                        "quoted_data": "'\\\"'"
                    }
                ],
                "output_details": []
            }
        },
        "job_3_load_to_hive": {
            "spark_engine": {
                "spark_run_mode": "medium",
                "retries": "0",
                "pause_idle_cluster_termination": "before_app",
                "scripts": [
                    "digital_merch/customer_attributes_table_load.sql"
                ],
                "input_details": [],
                "output_details": []
            }
        }
    }
}