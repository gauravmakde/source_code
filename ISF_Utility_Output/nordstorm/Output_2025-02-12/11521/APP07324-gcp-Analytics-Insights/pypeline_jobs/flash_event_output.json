{
    "dag_id": "flash_event_output",
    "event_start": "pendulum.datetime(2024, 2, 27, 16, 0)",
    "dag_schedule": "0 18 * * *",
    "prod_email_on_failure": "cia-app09142-customer-and-merch-attributes-to-object@nordstrom.pagerduty.com, jingnan.miao@nordstrom.com, yan.zhang@nordstrom.com",
    "nonprod_email_on_failure": "jingnan.miao@nordstrom.com, yan.zhang@nordstrom.com",
    "stages": [
        "update_flash_event_output_table",
        "tpt_export"
    ],
    "stage_dependencies": {
        "update_flash_event_output_table": [
            "tpt_export"
        ]
    },
    "update_flash_event_output_table": {
        "job_1_run_sql_to_update_t2_table": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_DMF",
                        "scripts": [
                            "flash_attribution/flash_event_output.sql"
                        ]
                    }
                ]
            }
        }
    },
    "tpt_export": {
        "job_2_tpt_export": {
            "tpt_engine": {
                "retries": "0",
                "input_details": [
                    {
                        "conn_name": "TPT_DMF",
                        "table_name": "flash_event_output_td_to_s3",
                        "s3_file_path": "s3://{s3_bucket_root_var}/tpt_export/flash_event_output_td_to_s3.csv",
                        "sql_file_name": "flash_attribution/tpt_export/flash_event_output_td_to_s3.sql",
                        "text_delimiter_hexa_flag": "N",
                        "text_delimiter": ",",
                        "file_count": "1",
                        "quoted_flag": "Y",
                        "quoted_data": "'\\\"'"
                    }
                ],
                "output_details": []
            }
        },
        "job_3_load_to_hive": {
            "spark_engine": {
                "spark_run_mode": "medium",
                "retries": "0",
                "pause_idle_cluster_termination": "before_app",
                "scripts": [
                    "flash_attribution/flash_event_output_load.sql"
                ],
                "input_details": [],
                "output_details": []
            }
        }
    }
}