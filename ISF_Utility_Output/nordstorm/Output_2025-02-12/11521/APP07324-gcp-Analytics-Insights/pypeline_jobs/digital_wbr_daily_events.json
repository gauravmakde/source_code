{
    "dag_id": "digital_wbr_daily_events",
    "dag_schedule": "0 21 * * *",
    "nonprod_email_on_failure": "sara.scott@nordstrom.com",
    "prod_email_on_failure": "dsa-app08231-dcj@nordstrom.pagerduty.com, sara.scott@nordstrom.com",
    "sql_env_vars": {
        "start_date": "current_date()-7",
        "end_date": "current_date()",
        "year": "2024"
    },
    "stages": [
        "dig_wbr_create_td_ldg_tables",
        "dig_wbr_load_to_csv",
        "dig_wbr_ldg_to_t2",
        "tpt_load_digital_wbr_daily_events_ldg"
    ],
    "stage_dependencies": {
        "dig_wbr_create_td_ldg_tables": "dig_wbr_load_to_csv",
        "dig_wbr_load_to_csv": [
            "tpt_load_digital_wbr_daily_events_ldg"
        ],
        "tpt_load_digital_wbr_daily_events_ldg": "dig_wbr_ldg_to_t2"
    },
    "dig_wbr_create_td_ldg_tables": {
        "job_1_create_td_landing_tables": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_MER",
                        "scripts": [
                            "digital_wbr/dig_wbr_tpt_job_insert.sql",
                            "digital_wbr/ddl/ddl_digital_wbr_daily_events_ldg.sql"
                        ]
                    }
                ]
            }
        }
    },
    "dig_wbr_load_to_csv": {
        "job_2_load_data_to_csv": {
            "spark_engine": {
                "spark_run_mode": "medium_no_broadcast",
                "retries": "0",
                "pause_idle_cluster_termination": "before_app",
                "scripts": [
                    "digital_wbr/digital_wbr_daily_events_load.sql"
                ],
                "input_details": [],
                "output_details": [
                    {
                        "conn_name": "TPT_CSV_OUTPUT",
                        "sql_table_reference": "digital_wbr_daily_events_ldg_output"
                    }
                ]
            }
        }
    },
    "tpt_load_digital_wbr_daily_events_ldg": {
        "tpt_dig_wbr_digital_wbr_daily_events_ldg": {
            "tpt_engine": {
                "retries": "0",
                "input_details": [
                    {
                        "conn_name": "TPT_MER",
                        "delimiter": "1F",
                        "table_name": "digital_wbr_daily_events_ldg",
                        "s3_file_path": "s3://{tpt_path}/digital_wbr_daily_events_ldg_output/*.csv.gz",
                        "job_name": "dig_wbr_digital_wbr_daily_events_ldg"
                    }
                ],
                "output_details": []
            }
        }
    },
    "dig_wbr_ldg_to_t2": {
        "job_3_update_t2_tables_from_landing_table": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "TERADATA_SOURCE_MER",
                        "scripts": [
                            "digital_wbr/digital_wbr_daily_events.sql"
                        ]
                    }
                ]
            }
        }
    }
}