{
    "dag_id": "customer_preference_daily_kafka_to_s3",
    "dag_schedule": "00 12 * * *",
    "dag_sla_minutes": "180",
    "prod_email_on_failure": "nap-marketing@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "",
    "stages": [
        "load_cpp_preference_daily_object_model",
        "extracted_and_audit_files_copy"
    ],
    "stage_dependencies": {
        "load_cpp_preference_daily_object_model": [
            "extracted_and_audit_files_copy"
        ],
        "extracted_and_audit_files_copy": [
            "save_status_of_the_dag"
        ]
    },
    "load_cpp_preference_daily_object_model": {
        "kafka_to_s3": {
            "spark_engine": {
                "spark_run_mode": "low",
                "scripts": [
                    "cpp_preference_daily_kafka_to_s3.sql"
                ],
                "input_details": [
                    {
                        "conn_name": "KAFKA_DAS_SINK",
                        "sql_table_reference": "kafka_source",
                        "data_source_name": "cpp_object_model_avro"
                    }
                ],
                "output_details": [
                    {
                        "conn_name": "__S3_CPP_PREFERENCE_DAILY_DATA_CSV_SINK__",
                        "sql_table_reference": "cpp_preference_data_extract"
                    },
                    {
                        "conn_name": "__S3_CPP_PREFERENCE_DAILY_AUDIT_CSV_SINK__",
                        "sql_table_reference": "cpp_preference_final_count"
                    },
                    {
                        "conn_name": "__S3_CPP_PREFERENCE_DAILY_CHANNEL_BRAND_DATA_CSV_SINK__",
                        "sql_table_reference": "cpp_preference_channel_brand_data_extract"
                    },
                    {
                        "conn_name": "__S3_CPP_PREFERENCE_DAILY_CHANNEL_BRAND_AUDIT_CSV_SINK__",
                        "sql_table_reference": "cpp_preference_channel_brand_final_count"
                    },
                    {
                        "conn_name": "__S3_CPP_PREFERENCE_DAILY_CONTENT_DATA_CSV_SINK__",
                        "sql_table_reference": "cpp_preference_content_data_extract"
                    },
                    {
                        "conn_name": "__S3_CPP_PREFERENCE_DAILY_CONTENT_AUDIT_CSV_SINK__",
                        "sql_table_reference": "cpp_preference_content_final_count"
                    },
                    {
                        "conn_name": "__S3_CPP_PREFERENCE_DAILY_CONTACT_DATA_CSV_SINK__",
                        "sql_table_reference": "cpp_preference_contact_data_extract"
                    },
                    {
                        "conn_name": "__S3_CPP_PREFERENCE_DAILY_CONTACT_AUDIT_CSV_SINK__",
                        "sql_table_reference": "cpp_preference_contact_final_count"
                    }
                ]
            }
        }
    },
    "extracted_and_audit_files_copy": {
        "to_s3_path_folders": {
            "python_executor": {
                "module": "python/utils",
                "version": "1.3.18",
                "args": [
                    "python/utils/src/bulk_s3_copy.py",
                    "--s3_source_bucket_env_name",
                    "S3_MKTG_BUCKET",
                    "--s3_target_bucket_env_name",
                    "S3_MKTG_BUCKET",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/data/cpp_preference/preference/cpp_preference_data_extract/', 'target': 'isf/data/cpp_preference/preference/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/audit/cpp_preference/preference/cpp_preference_final_count/', 'target': 'isf/audit/cpp_preference/preference/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/data/cpp_preference/channel_brand_preferences/cpp_preference_channel_brand_data_extract/', 'target': 'isf/data/cpp_preference/channel_brand_preferences/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/audit/cpp_preference/channel_brand_preferences/cpp_preference_channel_brand_final_count/', 'target': 'isf/audit/cpp_preference/channel_brand_preferences/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/data/cpp_preference/content_preferences/cpp_preference_content_data_extract/', 'target': 'isf/data/cpp_preference/content_preferences/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/audit/cpp_preference/content_preferences/cpp_preference_content_final_count/', 'target': 'isf/audit/cpp_preference/content_preferences/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/data/cpp_preference/contact_preferences/cpp_preference_contact_data_extract/', 'target': 'isf/data/cpp_preference/contact_preferences/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/audit/cpp_preference/contact_preferences/cpp_preference_contact_final_count/', 'target': 'isf/audit/cpp_preference/contact_preferences/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--base_date_iso_env_name",
                    "DAG_EXECUTION_DATE"
                ]
            },
            "macros": {
                "DAG_EXECUTION_DATE": "execution_date"
            }
        }
    }
}