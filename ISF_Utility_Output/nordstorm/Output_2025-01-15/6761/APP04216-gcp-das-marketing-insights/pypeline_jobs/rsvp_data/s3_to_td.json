{
    "dag_id": "rsvp_onetime_load",
    "dag_schedule": "00 22 * * *",
    "prod_email_on_failure": "nap-marketing@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "jianyuan.gan@nordstrom.com",
    "stages": [
        "clear_ldg_data",
        "read_s3_to_teradata"
    ],
    "stage_dependencies": {
        "clear_ldg_data": [
            "read_s3_to_teradata"
        ]
    },
    "clear_ldg_data": {
        "job_0": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "__Teradata_NAP_Source__",
                        "scripts": [
                            "clear_ldg.sql"
                        ]
                    }
                ]
            }
        }
    },
    "read_s3_to_teradata": {
        "job_0": {
            "spark_engine": {
                "spark_run_mode": "low",
                "scripts": [
                    "s3_to_stg.sql"
                ],
                "input_details": [
                    {
                        "conn_name": "__S3_RSVP_DATA_CSV__",
                        "sql_table_reference": "s3_read_rsvp_tbl"
                    }
                ],
                "output_details": [
                    {
                        "conn_name": "__Teradata_NAP_Source__",
                        "sql_table_reference": "write_teradata_rsvp_tbl",
                        "data_source_name": "RSVP_STG"
                    }
                ]
            }
        },
        "job_1": {
            "teradata_engine": {
                "input_details": [
                    {
                        "conn_name": "__Teradata_NAP_Source__",
                        "scripts": [
                            "stg_to_fct.sql"
                        ]
                    }
                ]
            }
        }
    }
}