{
    "dag_id": "contact_event_sendlog_kafka_to_s3",
    "dag_schedule": "00 01 * * *",
    "dag_sla_minutes": "180",
    "prod_email_on_failure": "nap-marketing@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "",
    "dependencies": {
        "upstream": {
            "dags": [
                {
                    "external_dag_id": "cpp_unsubscribe_kafka_to_s3_6761_DAS_MARKETING_das_marketing_insights",
                    "retries": 1,
                    "time_out": "5400"
                },
                {
                    "external_dag_id": "contact_event_kafka_to_s3_6761_DAS_MARKETING_das_marketing_insights",
                    "retries": 1,
                    "time_out": "5400"
                }
            ]
        }
    },
    "stages": [
        "load_contact_event_sendlog_object_model",
        "extracted_and_audit_files_copy"
    ],
    "stage_dependencies": {
        "load_contact_event_sendlog_object_model": [
            "extracted_and_audit_files_copy"
        ],
        "extracted_and_audit_files_copy": [
            "save_status_of_the_dag"
        ]
    },
    "load_contact_event_sendlog_object_model": {
        "kafka_to_s3": {
            "spark_engine": {
                "spark_run_mode": "low",
                "scripts": [
                    "contact_event_sendlog_kafka_to_s3.sql"
                ],
                "input_details": [
                    {
                        "conn_name": "KAFKA_DAS_SINK",
                        "sql_table_reference": "kafka_source",
                        "data_source_name": "marketing-contact-event-sendlog-recorded-avro"
                    }
                ],
                "output_details": [
                    {
                        "conn_name": "__S3_CONTACT_EVENT_SENDLOG_DATA_CSV_SINK__",
                        "sql_table_reference": "contact_event_sendlog_data_extract"
                    },
                    {
                        "conn_name": "__S3_CONTACT_EVENT_SENDLOG_AUDIT_CSV_SINK__",
                        "sql_table_reference": "contact_event_sendlog_final_count"
                    }
                ]
            }
        }
    },
    "extracted_and_audit_files_copy": {
        "to_s3_path_folder": {
            "python_executor": {
                "module": "python/utils",
                "version": "1.3.18",
                "args": [
                    "python/utils/src/bulk_s3_copy.py",
                    "--s3_source_bucket_env_name",
                    "S3_MKTG_BUCKET",
                    "--s3_target_bucket_env_name",
                    "S3_MKTG_BUCKET",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/data/contact_event_sendlog/contact_event_sendlog_data_extract/', 'target': 'isf/data/contact_event_sendlog/contact_event_send_log/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/audit/contact_event_sendlog/contact_event_sendlog_final_count/', 'target': 'isf/audit/contact_event_sendlog/contact_event_send_log/year=<year>/month=<month>/day=<day>/'}",
                    "--base_date_iso_env_name",
                    "DAG_EXECUTION_DATE"
                ]
            },
            "macros": {
                "DAG_EXECUTION_DATE": "execution_date"
            }
        }
    }
}