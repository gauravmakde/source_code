{
    "dag_id": "giftcard_dynamic_dims_kafka_to_s3",
    "dag_schedule": "00 22 * * *",
    "dag_sla_minutes": "75",
    "prod_email_on_failure": "nap-marketing@nordstrom.pagerduty.com",
    "nonprod_email_on_failure": "",
    "stages": [
        "load_alt_merchant",
        "load_funding",
        "load_merchant",
        "load_promotion",
        "extracted_and_audit_files_copy",
        "save_status_of_the_dag"
    ],
    "stage_dependencies": {
        "load_alt_merchant": [
            "extracted_and_audit_files_copy"
        ],
        "load_funding": [
            "extracted_and_audit_files_copy"
        ],
        "load_merchant": [
            "extracted_and_audit_files_copy"
        ],
        "load_promotion": [
            "extracted_and_audit_files_copy"
        ],
        "extracted_and_audit_files_copy": [
            "save_status_of_the_dag"
        ]
    },
    "load_alt_merchant": {
        "kafka_to_s3": {
            "spark_engine": {
                "spark_run_mode": "low",
                "scripts": [
                    "giftcard_dynamic_alt_merchant.sql"
                ],
                "input_details": [
                    {
                        "conn_name": "KAFKA_DAS_SINK",
                        "sql_table_reference": "kafka_source",
                        "data_source_name": "customer-giftcard-alternate-merchant-analytical-avro"
                    }
                ],
                "output_details": [
                    {
                        "conn_name": "__S3_GIFTCARD_DYNAMIC_DIMS_ALT_MERCHANT_DATA_CSV_SINK__",
                        "sql_table_reference": "alt_merchant_data_extract"
                    },
                    {
                        "conn_name": "__S3_GIFTCARD_DYNAMIC_DIMS_ALT_MERCHANT_AUDIT_CSV_SINK__",
                        "sql_table_reference": "alt_merchant_final_count"
                    }
                ]
            }
        }
    },
    "load_funding": {
        "kafka_to_s3": {
            "spark_engine": {
                "spark_run_mode": "low",
                "scripts": [
                    "giftcard_dynamic_funding.sql"
                ],
                "input_details": [
                    {
                        "conn_name": "KAFKA_DAS_SINK",
                        "sql_table_reference": "kafka_source",
                        "data_source_name": "customer-giftcard-funding-type-analytical-avro"
                    }
                ],
                "output_details": [
                    {
                        "conn_name": "__S3_GIFTCARD_DYNAMIC_DIMS_FUNDING_DATA_CSV_SINK__",
                        "sql_table_reference": "funding_data_extract"
                    },
                    {
                        "conn_name": "__S3_GIFTCARD_DYNAMIC_DIMS_FUNDING_AUDIT_CSV_SINK__",
                        "sql_table_reference": "funding_final_count"
                    }
                ]
            }
        }
    },
    "load_merchant": {
        "kafka_to_s3": {
            "spark_engine": {
                "spark_run_mode": "low",
                "scripts": [
                    "giftcard_dynamic_merchant.sql"
                ],
                "input_details": [
                    {
                        "conn_name": "KAFKA_DAS_SINK",
                        "sql_table_reference": "kafka_source",
                        "data_source_name": "customer-giftcard-merchant-analytical-avro"
                    }
                ],
                "output_details": [
                    {
                        "conn_name": "__S3_GIFTCARD_DYNAMIC_DIMS_MERCHANT_DATA_CSV_SINK__",
                        "sql_table_reference": "merchant_data_extract"
                    },
                    {
                        "conn_name": "__S3_GIFTCARD_DYNAMIC_DIMS_MERCHANT_AUDIT_CSV_SINK__",
                        "sql_table_reference": "merchant_final_count"
                    }
                ]
            }
        }
    },
    "load_promotion": {
        "kafka_to_s3": {
            "spark_engine": {
                "spark_run_mode": "low",
                "scripts": [
                    "giftcard_dynamic_promotion.sql"
                ],
                "input_details": [
                    {
                        "conn_name": "KAFKA_DAS_SINK",
                        "sql_table_reference": "kafka_source",
                        "data_source_name": "customer-giftcard-promotion-analytical-avro"
                    }
                ],
                "output_details": [
                    {
                        "conn_name": "__S3_GIFTCARD_DYNAMIC_DIMS_PROMOTION_DATA_CSV_SINK__",
                        "sql_table_reference": "promotion_data_extract"
                    },
                    {
                        "conn_name": "__S3_GIFTCARD_DYNAMIC_DIMS_PROMOTION_AUDIT_CSV_SINK__",
                        "sql_table_reference": "promotion_final_count"
                    }
                ]
            }
        }
    },
    "extracted_and_audit_files_copy": {
        "to_s3_path_folders": {
            "python_executor": {
                "module": "python/utils",
                "version": "1.3.18",
                "args": [
                    "python/utils/src/bulk_s3_copy.py",
                    "--s3_source_bucket_env_name",
                    "S3_MKTG_BUCKET",
                    "--s3_target_bucket_env_name",
                    "S3_MKTG_BUCKET",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/data/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_alt_merchant/alt_merchant_data_extract/', 'target': 'isf/data/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_alt_merchant/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/audit/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_alt_merchant/alt_merchant_final_count/', 'target': 'isf/audit/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_alt_merchant/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/data/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_funding_class/funding_data_extract/', 'target': 'isf/data/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_funding_class/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/audit/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_funding_class/funding_final_count/', 'target': 'isf/audit/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_funding_class/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/data/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_merchant/merchant_data_extract/', 'target': 'isf/data/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_merchant/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/audit/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_merchant/merchant_final_count/', 'target': 'isf/audit/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_merchant/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/data/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_promotion/promotion_data_extract/', 'target': 'isf/data/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_promotion/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--source_to_target_path_pair",
                    "{'source': 'isf/audit/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_promotion/promotion_final_count/', 'target': 'isf/audit/gift_card_marketing/daily/gift_card_dynamic_dims/gift_card_promotion/incremental/year=<year>/month=<month>/day=<day>/'}",
                    "--base_date_iso_env_name",
                    "DAG_EXECUTION_DATE"
                ]
            },
            "macros": {
                "DAG_EXECUTION_DATE": "execution_date"
            }
        }
    },
    "save_status_of_the_dag": {
        "to_file_on_s3": {
            "python_executor": {
                "module": "python/utils",
                "version": "1.3.18",
                "args": [
                    "python/utils/src/save_status_of_the_dag.py",
                    "--s3_bucket",
                    "S3_MKTG_BUCKET",
                    "--dag_name",
                    "giftcard_dynamic_dims_kafka_to_s3",
                    "--base_date_iso_env_name",
                    "DAG_EXECUTION_DATE"
                ]
            },
            "macros": {
                "DAG_EXECUTION_DATE": "execution_date"
            }
        }
    }
}