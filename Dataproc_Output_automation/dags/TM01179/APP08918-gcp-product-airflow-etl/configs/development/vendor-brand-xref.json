            , "VendorBrandOrderFromAssociation.VendorNumber vendorbrandorderfromassociation_vendornumber"
            , "VendorBrandOrderFromAssociation.AssociationStatus vendorbrandorderfromassociation_associationstatus"
            , "cast(VendorBrandOrderFromAssociation.VendorBrand.Code as string) vendorbrandorderfromassociation_vendorbrand_code"
