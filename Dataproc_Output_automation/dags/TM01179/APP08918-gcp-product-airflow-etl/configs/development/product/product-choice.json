            , "NordstromDisplayColor nordstromdisplaycolor"
            , "ParentEPMStyleId parentepmstyleid"
            , "ProductAction productaction"
            , "ProductActionTimestamp productactiontimestamp"
            , "RmsStyleGroupId rmsstylegroupid"
